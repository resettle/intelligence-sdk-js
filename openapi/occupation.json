{
  "openapi": "3.1.0",
  "info": {
    "title": "Resettle Occupation API",
    "description": "Seamlessly translate occupation codes across 10 global classification systems. Our Occupation API provides instant crosswalk mappings between ISCO, USSOC, NOC, ANZSCO, UKSOC, and more, supporting international hiring, immigration assessments, and labor market analysis. Search and query thousands of standardized occupation codes with fuzzy matching, enabling your applications to understand and match careers across borders. Perfect for HR platforms, immigration tech, and workforce analytics solutions.",
    "version": "1",
    "termsOfService": "https://resettle.ai/terms",
    "contact": {
      "email": "contact@resettle.ai"
    },
    "license": {
      "name": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  "externalDocs": {
    "description": "Try our APIs out",
    "url": "https://resettle.ai/dev"
  },
  "servers": [
    {
      "url": "https://resettle-occupation-api.p.rapidapi.com/occupation"
    }
  ],
  "security": [
    {
      "RapidAuth": []
    }
  ],
  "components": {
    "securitySchemes": {
      "RapidAuth": {
        "type": "apiKey",
        "in": "header",
        "description": "If you don't currently have an API key, you can visit https://resettle.ai/dev to acquire a free one",
        "name": "X-RapidAPI-Key"
      }
    },
    "parameters": {
      "RapidHost": {
        "name": "X-RapidAPI-Host",
        "in": "header",
        "required": true,
        "schema": {
          "type": "string",
          "enum": [
            "resettle-occupation-api.p.rapidapi.com"
          ]
        }
      }
    }
  },
  "paths": {
    "/occupation/crosswalk": {
      "get": {
        "operationId": "getOccupationCrosswalk",
        "description": "Crosswalk occupation codes between classifications",
        "responses": {
          "200": {
            "description": "Occupation codes crosswalked successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "code": {
                      "type": "integer",
                      "minimum": -9007199254740991,
                      "maximum": 9007199254740991
                    },
                    "message": {
                      "type": "string"
                    },
                    "data": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "description": "The id of the occupation code in format {classification}-{code}",
                            "type": "string"
                          },
                          "classification": {
                            "description": "The occupation classification",
                            "type": "string",
                            "enum": [
                              "isco-2008",
                              "ussoc-2018",
                              "uksoc-2020",
                              "nol-2024",
                              "noc-2021",
                              "osca-2024",
                              "ussoc-2010",
                              "noc-2016",
                              "anzsco-2013",
                              "anzsco-2022"
                            ]
                          },
                          "code": {
                            "description": "The code of the occupation under the classification",
                            "type": "string"
                          },
                          "label": {
                            "description": "The name of the occupation",
                            "type": "string"
                          }
                        },
                        "required": [
                          "id",
                          "classification",
                          "code",
                          "label"
                        ]
                      }
                    }
                  },
                  "required": [
                    "code",
                    "message",
                    "data"
                  ]
                },
                "examples": {
                  "isco-2008-2512-to-ussoc-2018": {
                    "summary": "Crosswalk from ISCO-08 2512 to U.S. SOC 2018",
                    "value": {
                      "code": 0,
                      "message": "Success",
                      "data": [
                        {
                          "id": "ussoc-2018-15-1252",
                          "classification": "ussoc-2018",
                          "code": "15-1252",
                          "label": "Software Developers"
                        },
                        {
                          "id": "ussoc-2018-15-1253",
                          "classification": "ussoc-2018",
                          "code": "15-1253",
                          "label": "Software Quality Assurance Analysts and Testers"
                        }
                      ]
                    }
                  }
                }
              }
            }
          }
        },
        "parameters": [
          {
            "in": "query",
            "name": "from",
            "schema": {
              "anyOf": [
                {
                  "description": "The id of the occupation code in format {classification}-{code}",
                  "type": "string"
                },
                {
                  "type": "object",
                  "properties": {
                    "classification": {
                      "description": "The occupation classification",
                      "type": "string",
                      "enum": [
                        "isco-2008",
                        "ussoc-2018",
                        "uksoc-2020",
                        "nol-2024",
                        "noc-2021",
                        "osca-2024",
                        "ussoc-2010",
                        "noc-2016",
                        "anzsco-2013",
                        "anzsco-2022"
                      ]
                    },
                    "code": {
                      "description": "The code of the occupation under the classification",
                      "type": "string"
                    }
                  },
                  "required": [
                    "classification",
                    "code"
                  ]
                }
              ]
            },
            "required": true
          },
          {
            "in": "query",
            "name": "to",
            "schema": {
              "type": "string",
              "enum": [
                "isco-2008",
                "ussoc-2018",
                "uksoc-2020",
                "nol-2024",
                "noc-2021",
                "osca-2024",
                "ussoc-2010",
                "noc-2016",
                "anzsco-2013",
                "anzsco-2022"
              ]
            },
            "required": true,
            "description": "The occupation classification"
          }
        ]
      }
    },
