            }
          }
        },
        "parameters": [
          {
            "in": "query",
            "name": "classification",
            "schema": {
              "type": "string",
              "enum": [
                "isco-2008",
                "ussoc-2018",
                "uksoc-2020",
                "nol-2024",
                "noc-2021",
                "osca-2024",
                "ussoc-2010",
                "noc-2016",
                "anzsco-2013",
                "anzsco-2022"
              ]
            },
            "required": true,
            "description": "The occupation classification"
          },
          {
            "in": "query",
            "name": "code",
            "schema": {
              "type": "string"
            },
            "description": "The code of the occupation under the classification"
          },
          {
            "in": "query",
            "name": "cursor",
            "schema": {
              "type": "string"
            },
            "description": "The identifier of the page"
          },
          {
            "in": "query",
            "name": "limit",
            "schema": {
              "type": "integer",
              "minimum": 1,
              "maximum": 100
            },
            "description": "The max items to retrieve, default to 100"
          }
        ]
      }
    },
