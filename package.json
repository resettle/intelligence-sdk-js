{
  "name": "intelligence-sdk-js",
  "version": "0.0.0",
  "description": "Resettle Intelligence SDK for JavaScript",
  "private": true,
  "type": "module",
  "devDependencies": {
    "@changesets/cli": "2.29.8",
    "@typescript-eslint/eslint-plugin": "8.50.1",
    "@typescript-eslint/parser": "8.50.1",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-sort-export-all": "2.1.0",
    "prettier": "3.7.4"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:resettle/intelligence-sdk-js.git"
  },
  "author": {
    "name": "Resettle Team",
    "url": "https://resettle.ai"
  },
  "license": "Apache-2.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "packageManager": "pnpm@10.26.0+sha512.3b3f6c725ebe712506c0ab1ad4133cf86b1f4b687effce62a9b38b4d72e3954242e643190fc51fa1642949c735f403debd44f5cb0edd657abe63a8b6a7e1e402",
  "scripts": {
    "build": "pnpm -r --filter \"./packages/**\" build",
    "bump": "pnpm changeset version",
    "bump:ci": "pnpm changeset version",
    "lint": "eslint --cache --fix",
    "prettify": "prettier --write packages/*/src/**/*.ts",
    "publish:ci": "pnpm -s --filter \"./packages/**\" publish -r --access public"
  }
}
